{
  "all": true,
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "**/*.d.ts",
    "**/*.test.ts",
    "**/index.ts",
    "test/**",
    "dist/**",
    "node_modules/**"
  ],
  "reporter": [
    "text",
    "lcov",
    "html"
  ],
  "branches": 80,
  "lines": 80,
  "functions": 80,
  "statements": 80
}