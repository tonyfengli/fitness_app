{
  "name": "@acme/ai",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "lint": "eslint .",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "generate": "tsx src/run-workout-generator.ts",
    "generate-workout": "pnpm generate",
    "test": "NODE_ENV=test vitest",
    "test:ui": "NODE_ENV=test vitest --ui",
    "test:coverage": "NODE_ENV=test vitest run --coverage",
    "test:integration": "NODE_ENV=test vitest run test/integration",
    "test:unit": "NODE_ENV=test vitest run test/unit",
    "test:watch": "NODE_ENV=test vitest --watch"
  },
  "dependencies": {
    "@acme/db": "workspace:*",
    "@langchain/core": "^0.3.26",
    "@langchain/langgraph": "^0.3.6",
    "@langchain/openai": "^0.5.16",
    "dotenv": "^16.4.7",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@acme/eslint-config": "workspace:*",
    "@acme/prettier-config": "workspace:*",
    "@acme/tsconfig": "workspace:*",
    "@testing-library/jest-dom": "^6.6.3",
    "@vitest/ui": "^3.2.4",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsx": "^4.19.2",
    "typescript": "catalog:",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^3.1.0"
  },
  "prettier": "@acme/prettier-config"
}